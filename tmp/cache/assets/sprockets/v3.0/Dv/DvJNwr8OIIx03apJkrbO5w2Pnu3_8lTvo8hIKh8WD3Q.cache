I"(function() {
  $(document).ready(function() {
    var magicNav, nav, nav_height;
    nav = $("nav", "#site_wrapper");
    nav_height = nav.height();
    $(window).on("load", function() {
      var win_top_above;
      return win_top_above = true;
    });
    $(window).on("load scroll", function() {
      var win_top, win_top_above;
      win_top = $(this).scrollTop();
      if ((win_top > nav_height && win_top_above) || (win_top < nav_height && !win_top_above)) {
        win_top_above = magicNav(win_top_above);
        console.log(magicNav(win_top_above));
      }
    });
    magicNav = function(win_top_above) {
      if (win_top_above) {
        nav.css({
          "background-color": "transparent"
        });
        $(".navbar-brand", nav).css({
          "display": "none"
        });
      } else {
        nav.css({
          "background-color": "initial"
        });
        $(".navbar-brand", nav).css({
          "display": "inline-block"
        });
      }
      return !win_top_above;
    };
  });

}).call(this);
