I"¤(function() {
  $(document).ready(function() {
    var nav, nav_height;
    nav = $("nav", "#site_wrapper");
    nav_height = nav.height();
    $(window).on("load", function() {
      var win_top_above;
      return win_top_above = false;
    });
    return $(window).on("load scroll", function() {
      var win_top, win_top_above;
      win_top = $(this).scrollTop();
      if (win_top > nav_height && win_top_above) {
        console.log("GOING DOWN");
        nav.css({
          "background-color": "initial"
        });
        $(".navbar-brand", nav).css({
          "display": "inline-block"
        });
      } else if (win_top < nav_height && !win_top_above) {
        console.log("GOING UP");
        nav.css({
          "background-color": "transparent"
        });
        $(".navbar-brand", nav).css({
          "display": "none"
        });
      }
      win_top_above = !win_top_above;
    });
  });

}).call(this);
